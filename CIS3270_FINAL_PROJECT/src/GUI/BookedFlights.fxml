<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="FlightPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.BookedFlightController">
   <children>
      <Button fx:id="selectFlights" alignment="CENTER_RIGHT" contentDisplay="CENTER" mnemonicParsing="false" onAction="#popUpDetails" prefHeight="107.0" prefWidth="655.0" stylesheets="@UserHome.css">
         <graphic>
            <HBox alignment="CENTER" prefHeight="98.0" prefWidth="641.0" spacing="5.0">
               <children>
                  <Label fx:id="FlightID" alignment="CENTER" prefHeight="23.0" prefWidth="87.0" text="LightID">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="Date" alignment="CENTER" prefHeight="23.0" prefWidth="113.0" text="Date">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="DepartingCity" alignment="CENTER" prefHeight="55.0" prefWidth="151.0" text="Departing City">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="ArrivingCity" alignment="CENTER" prefHeight="38.0" prefWidth="141.0" text="Arriving City">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="Time" prefHeight="23.0" prefWidth="85.0" text="Time">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="AirlineName" alignment="CENTER" prefHeight="26.0" prefWidth="69.0" text="Airline">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </graphic>
      </Button>
   </children>
</AnchorPane>
