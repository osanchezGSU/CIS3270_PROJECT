<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="FlightId" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="GUI.BookedFlightController">
   <children>
      <HBox fx:id="FlightLayout" alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="34.0" prefWidth="658.0" spacing="10.0">
         <children>
            <Label fx:id="FlightID" alignment="CENTER" prefHeight="17.0" prefWidth="110.0" text="Flight ID">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="Date" alignment="CENTER" prefHeight="27.0" prefWidth="91.0" text="Date">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="DepartingCity" alignment="CENTER" prefHeight="27.0" prefWidth="155.0" text="Departing From">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="ArrivingCity" alignment="CENTER" prefHeight="27.0" prefWidth="108.0" text="Arriving To">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="Time" alignment="CENTER" prefHeight="17.0" prefWidth="94.0" text="Time">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="AirlineName" prefHeight="26.0" prefWidth="69.0" text="Airline">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button fx:id="DeleteButton" mnemonicParsing="false" prefHeight="41.0" prefWidth="35.0" styleClass="transparent" stylesheets="@UserHome.css">
               <graphic>
                  <ImageView fitHeight="28.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../../Downloads/Trash.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
